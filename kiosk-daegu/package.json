{
  "name": "touch-kiosk",
  "version": "1.0.0",
  "description": "어린이 등록 시스템",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --x64",
    "dist": "electron-builder --win --portable" 
  },
  "build": {
    "appId": "com.tinkerwish.kiosk",
